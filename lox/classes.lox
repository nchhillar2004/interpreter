class Arithmetic{
    init () {
        print "Arithmetic calculations";
    }

    add(a, b) {
        var sum = a + b;
        return sum;
    }
}

class Maths < Arithmetic {
    sub(a, b) {
        return a - b;
    }
}

var mth = Maths();
print mth.add(10, 2); // Sum: 12
print mth.sub(10, 2); // 8
